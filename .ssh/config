#Github Centrica
Host centrica.github.com
  HostName github.com
  User git                         
  IdentityFile ~/.ssh/id_rsa_github_centrica

#Github Personal
Host github.com
  HostName github.com
  UseKeychain yes
  User git
  IdentityFile ~/.ssh/id_rsa_github

#Pi-hole
Host pi-hole.local 
  HostName pi-hole.local
  User pi
  IdentityFile ~/.ssh/id_rsa_pihole
