
#Bitbucket
Host bb
  HostName bitbucket.org
  User fabio.dessi@clevertech.biz
  IdentityFile ~/.ssh/id_rsa_bitbucket

#Github 
Host Github
  HostName *.github.com
  User f.dessi@fabiodessi.dev
  IdentityFile ~/.ssh/id_rsa_github

#Pi-hole
Host Pi-hole 
  HostName pi-hole.local
  User pi
  IdentityFile ~/.ssh/id_rsa_pihole

#Default
Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa_bitbucket
  IdentityFile ~/.ssh/id_rsa_github
  IdentityFile ~/.ssh/id_rsa_pihole

