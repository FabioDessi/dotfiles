#Github Centrica
Host centrica.github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_centrica
  IdentitiesOnly yes
  AddKeysToAgent yes

#Azure Ascent
Host ssh.dev.azure.com 
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/id_ascent
  IdentitiesOnly yes
  AddKeysToAgent yes

#Github Personal
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_github
  UseKeychain yes
  IdentitiesOnly yes
  AddKeysToAgent yes

#Pi-hole
Host pi-hole.local 
  HostName pi-hole.local
  User pi
  IdentityFile ~/.ssh/id_pihole
  IdentitiesOnly yes
  AddKeysToAgent yes

