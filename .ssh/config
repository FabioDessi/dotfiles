Host *
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

#Azure Ascent
Host ssh.dev.azure.com 
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/id_ascent
  IdentitiesOnly yes
  AddKeysToAgent yes

#Github Personal
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_github
  UseKeychain yes
  IdentitiesOnly yes
  AddKeysToAgent yes
